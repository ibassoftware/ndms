<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_payslip_import_form" model="ir.ui.view">
        <field name="name">payslip.import.form</field>
        <field name="model">payslip.import</field>
        <field name="arch" type="xml">
            <form string="Payslips Import">
                <group>
                    <field name="file"/>
                </group>
                <group>
                    <group>
                        <field name="date_from"/>
                    </group>
                    <group>
                        <field name="date_to"/>
                    </group>
                </group>
                <footer>
                    <button string='Import' name="payslip_import" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
           </form>
        </field>
    </record>
</odoo>
